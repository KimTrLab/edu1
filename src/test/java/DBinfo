
CREATE TABLE user (
    id BIGINT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(10) unique key NOT NULL,
    password VARCHAR(60) NOT NULL,
    email VARCHAR(50) unique key,
    constraint CHK_USER_NAME CHECK (CHAR_LENGTH(name) >= 2),       -- 최소 4글자
    constraint CHK_USER_EMAIL CHECK (CHAR_LENGTH(password) >= 8)    -- 최소 8글자
);

create table Porder(
	id BIGINT primary key auto_increment,
	product varchar(10) not null,
	user_id BIGINT 
);



commit;
drop table user;
delete from user;
select * from user;

insert into user (name, email) values ('aaa','bbbb');
insert into user (name, email) values ('badaf','bbbb');
insert into user (name, email) values ('badca','bbbb');
commit;

delete from Porder;
drop table Porder;
insert into Porder (product,user_id) values ('오징어',5);
insert into Porder (product,user_id) values ('장어',5);
commit;
select * from Porder;